{
  "compilerOptions": {
    "noEmit": true,
    "lib": ["dom", "esnext"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": ["./types/*"]
    },
    "target": "es5",
    "experimentalDecorators": true
  }
}
